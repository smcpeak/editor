// lsp-start-server-other-language.ev
// Try to start an LSP server for a language we cannot handle.
{
  args: ["README.md"]
  cmds: [

// In editor with expected file.
CheckFocusWidget("window1.frame1.editorFrame.m_editorWidget")
CheckQuery("window1.frame1.editorFrame.m_editorWidget" "documentFileName" "README.md")

// Try to start LSP.
Shortcut("window1.m_menuBar" "Alt+L")
KeyPress("window1.m_menuBar.lspMenu" "Key_A" "a")

// Pops up error box.
CheckFocusWidget("window1.#5.qt_msgbox_buttonbox.#1")
CheckMessageBoxTextMatches("window1.#5", "This editor does not know how to run an LSP server for Plain text.")

// Dismiss dialog.
FocusKeyPR("Key_Return" "\r")

// Back in editor.
CheckFocusWidget("window1.frame1.editorFrame.m_editorWidget")

]}
// EOF
