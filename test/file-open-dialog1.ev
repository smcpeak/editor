# file-open-dialog1.ev
# Use the file-open dialog to open a file, using Tab completion once.

Shortcut "window1.m_menuBar.fileMenu.fileOpen" "F3"
KeyPress "filename_input.m_filenameEdit" "Key_A" "a"
KeyPress "filename_input.m_filenameEdit" "Key_Slash" "/"
KeyPress "filename_input.m_filenameEdit" "Key_B" "b"
KeyPress "filename_input.m_filenameEdit" "Key_Slash" "/"
KeyPress "filename_input.m_filenameEdit" "Key_F" "f"
KeyPress "filename_input.m_filenameEdit" "Key_O" "o"
KeyPress "filename_input.m_filenameEdit" "Key_Tab" ""
KeyPress "filename_input.m_filenameEdit" "Key_Return" ""
KeyPress "window1.frame1.editorFrame.m_editorWidget" "Shift+Key_Down" ""
Shortcut "window1.m_menuBar.editMenu.editCopy" "Ctrl+C"

CheckClipboard "foo\n"

# EOF
